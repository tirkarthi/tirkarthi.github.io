---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>A Technical Blog on Clojure</title>
    <description>Clojure posts by Karthikeyan S</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ "/feed_clojure.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    {% for post in site.categories.clojure %}
    <item>
      <title>{{ post.title | xml_escape }}</title>
      <description>{{ post.content | xml_escape }}</description>
      <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
      <link>{{ post.url | prepend: site.url }}</link>
      <guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
    </item>
    {% endfor %}
  </channel>
</rss>
